<?xml version="1.0" encoding="UTF-8"?><xsd:schema targetNamespace="http://schemas.GlennWood.us/Benchmark/MemoryUsage" xmlns="http://schemas.GlennWood.us/Benchmark/MemoryUsage" xmlns:xsd="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified">	<xsd:element name="MemoryUsage">		<xsd:complexType>			<xsd:sequence>				<xsd:element name="T" type="T" minOccurs="1" maxOccurs="unbounded"/>			</xsd:sequence>			<xsd:attribute name="n" type="xsd:string" use="required"/>			<xsd:attribute name="tm" type="xsd:string" use="required"/>			<xsd:attribute name="pid" type="xsd:short" use="required"/>			<xsd:attribute name="userid" type="xsd:string" use="required"/>			<xsd:attribute name="os" type="xsd:string" use="required"/>		</xsd:complexType>	</xsd:element>	<xsd:complexType name="T">		<xsd:sequence>			<xsd:element name="V" type="V" minOccurs="1" maxOccurs="unbounded"/>		</xsd:sequence>		<xsd:attribute name="t" use="required" type="TM"/>		<xsd:attribute name="p" type="PackageName"/>		<xsd:attribute name="f" type="xsd:string"/>		<xsd:attribute name="l">			<xsd:simpleType>				<xsd:restriction base="xsd:integer"/>			</xsd:simpleType>		</xsd:attribute>		<xsd:attribute name="ps" type="xsd:string" use="required"/>		<xsd:attribute name="n" type="SubroutineName" use="required"/>	</xsd:complexType>	<xsd:complexType name="V">		<xsd:attribute name="n" use="required" type="ValueName"/>		<xsd:attribute name="s" use="required" type="xsd:integer"/>	</xsd:complexType>	<xsd:simpleType name="PackageName">		<xsd:restriction base="xsd:string">			<xsd:pattern value="([\w\d_]+::)*[\w\d_]+"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="SubroutineName">		<xsd:restriction base="xsd:string">			<xsd:pattern value="[\w\d_]+\(([\w\d_]+::)*[\w\d_]+\)"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="ValueName">		<xsd:restriction base="xsd:string">			<xsd:pattern value="(([\w\d_]+::)*[\w\d_]+)|(\d+)"/>		</xsd:restriction>	</xsd:simpleType>	<xsd:simpleType name="TM">		<xsd:restriction base="xsd:float"/>	</xsd:simpleType></xsd:schema>